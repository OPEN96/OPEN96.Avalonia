<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net8.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Title>OPEN96.Avalonia</Title>
        <Description>OPEN96.Avalonia</Description>
        <Copyright>OPEN96</Copyright>
        <PackageProjectUrl>https://github.com/OPEN96/OPEN96.Avalonia</PackageProjectUrl>
        <PackageLicenseUrl>https://github.com/OPEN96/OPEN96.Avalonia</PackageLicenseUrl>
        <RepositoryUrl>https://github.com/OPEN96/OPEN96.Avalonia.git</RepositoryUrl>
        <AssemblyVersion>1.0.0</AssemblyVersion>
        <FileVersion>1.0.0</FileVersion>
        <PackageReadmeFile>README.md</PackageReadmeFile>
        <PackageReleaseNotes>OPEN96.Avalonia</PackageReleaseNotes>
        <PackageTags>OPEN96.Avalonia</PackageTags>
    </PropertyGroup>

    <ItemGroup>
      <None Include="..\README.md">
        <Pack>True</Pack>
        <PackagePath>\</PackagePath>
      </None>
    </ItemGroup>

    <ItemGroup>
      <PackageReference Include="Avalonia" Version="11.2.5" />
    </ItemGroup>

    <ItemGroup>
      <Folder Include="Controls\" />
    </ItemGroup>

</Project>
